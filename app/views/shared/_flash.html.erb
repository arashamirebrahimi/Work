<% if flash.notice %>
    <div class="alert alert-info mt-4" role = "alert">
    <!-- Look up alerts in bootstrap
    adding ".inspect" will print the content -->
        <%= flash.notice %>
    </div>
<% end %>
<% if flash.alert %>
    <div class="alert alert-warning mt-4" role="alert">
        <%= flash.alert %>
    </div>
<% end %>